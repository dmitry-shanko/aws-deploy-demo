[Unit]
Description=Trade Signals App
After=network.target

[Service]
User=ec2-user
EnvironmentFile=$JVM_OPTIONS_FULL_PATH
ExecStart=/usr/bin/java -jar $AMI_JAR_PATH
SuccessExitStatus=143
Restart=on-failure
RestartSec=10
StandardOutput=null
StandardError=null
SyslogIdentifier=trade-signals

[Install]
WantedBy=multi-user.target
